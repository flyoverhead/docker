---

apiVersion: 1

datasources:
  - name: {{ grafana_source.name }}
    type: {{ grafana_source.type }}
    access: proxy
    orgId: 1
    url: {{ grafana_source.url }}:{{ grafana_source.port }}
    basicAuth: false
    isDefault: true
    jsonData:
      graphiteVersion: "1.1"
      tlsAuth: false
      tlsAuthWithCACert: false
    secureJsonData:
      tlsCACert: "..."
      tlsClientCert: "..."
      tlsClientKey: "..."
    version: 1
    editable: true
